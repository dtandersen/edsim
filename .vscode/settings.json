{
    "java.configuration.updateBuildConfiguration": "automatic",
    "java.compile.nullAnalysis.mode": "automatic",
    "editor.inlayHints.enabled": "off",
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "java.sources.organizeImports.starThreshold": 1,
    "java.sources.organizeImports.staticStarThreshold": 99,
    "java.format.settings.url": ".vscode/java-formatter.xml",
    "java.format.settings.profile": "David",
    "[java]": {
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": "always"
        },
    },
    "testing.automaticallyOpenPeekView": "never",
    "testing.autoRun.mode": "onlyPreviousRun",
    "java.completion.favoriteStaticMembers": [
        "edu.ucsd.awsed.matcher.AwsedMatchers.*",
        "org.junit.Assert.*",
        "org.junit.Assume.*",
        "org.junit.jupiter.api.Assertions.*",
        "org.junit.jupiter.api.Assumptions.*",
        "org.junit.jupiter.api.DynamicContainer.*",
        "org.junit.jupiter.api.DynamicTest.*",
        "org.mockito.Mockito.*",
        "org.mockito.ArgumentMatchers.*",
        "org.mockito.Answers.*",
        "org.hamcrest.Matchers.*",
        "org.hamcrest.MatcherAssert.*",
        "io.jenetics.engine.EvolutionResult.*",
    ],
    "java.debug.settings.onBuildFailureProceed": true,
    "java.test.defaultConfig": "base",
    "java.test.config": [
        {
            "name": "base",
            "envFile": "${workspaceFolder}/.env",
            "vmArgs": [
            ]
        },
    ],
    "java.maven.downloadSources": true,
    // "java.maven."
    // "java.maven.downloadJavadoc": true,
}
